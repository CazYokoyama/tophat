  <Button Name="cmdSelect" Caption="Select" X="0" Y="-70" Width="65" Height="35" OnClick="OnSelectClicked" />
  <Button Name="cmdClose" Caption="Cancel" X="0" Y="-35" Width="65" Height="35" OnClick="OnCloseClicked" />

  <List Name="frmWaypointList" X="65" Y="0" Width="-1" Height="-20" />
  <Label Name="frmSummaryLabels1" X="65" Width="50" Y="-20" Height="20"/>
  <Label Name="frmSummaryValues1" X="115" Y="-20" Height="20"/>
  <Label Name="frmSummaryLabels2" X="185" Width="50" Y="-20" Height="20"/>
  <Label Name="frmSummaryValues2" X="235" Y="-20" Width="70" Height="20"/>   
